generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

// Modèle User avec authentification
model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String   // Password hashé avec bcrypt
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
